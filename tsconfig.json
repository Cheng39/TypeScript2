{
  "compilerOptions": {
      "target": "ES5",
      "module": "umd",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "incremental": true,
      "declaration": true,
      "outDir": "./dist" // 确保输出目录正确
  },
  "include": ["src/**/*"],
  "exclude": ["dist"]
}